package firstproject;

public class Hammer {
  public boolean destroy(Phone p){
	  p.turnedOn = false;
	  p.price = 0;
	  System.out.println("BOOM");
	//return true;
	  
  }
  
  public static void main(String[] args){
	  
	  Hammer h = new Hammer();
	  Phone phone = new Phone();
	  iPhone phone2 = new iPhone();
	  
	  h.destroy(phone);
	  h.destroy(phone2);
  }
}
